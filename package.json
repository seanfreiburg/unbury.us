{
  "name": "unbury.us",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "vite build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:http": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/e2e/http.test.js --runInBand --forceExit",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/e2e/loan-calculator.test.js --runInBand",
    "test:all": "npm run test:unit && npm run test:http"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.3",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/handlebars": "^4.0.40",
    "@types/jquery": "^3.5.33",
    "@types/node": "^25.0.3",
    "eslint": "^8.57.1",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.7.4",
    "puppeteer": "^22.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  }
}
